	Journal = {Organization Science},
	Month = apr,
	Number = {2},
	Pages = {163 --182},
	Shorttitle = {Making Connections},
	Title = {Making Connections: Complementary Influences on Communication Media Choices, Attitudes, and Use},
	Volume = {11},
	Year = {2000},
	Bdsk-Url-1 = {http://orgsci.journal.informs.org/content/11/2/163.abstract},
