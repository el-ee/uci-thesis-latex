	Issn = {0028-3932},
	Journal = {Neuropsychologia},
	Keywords = {Conceptual system, Embodied cognition, Emotion, Grounded cognition, Situated action},
	Month = apr,
	Number = {5},
	Pages = {1105--1127},
	Title = {Grounding emotion in situated conceptualization},
