	Journal = {Annals of Internal Medicine},
	Number = {2},
	Pages = {123-132},
	Title = {Sedentary Time and Its Association With Risk for Disease Incidence, Mortality, and Hospitalization in AdultsA Systematic Review and Meta-analysisSedentary Time and Disease Incidence, Mortality, and Hospitalization},
