	Publisher = {SF-TH Inc},
	Title = {Daughter of Earth: Judith Merril and the Intersections of Gender, Science Fiction, and Frontier Mythology},
	Url = {http://www.jstor.org/stable/25475207},
	Volume = {36},
	Year = {2009},
