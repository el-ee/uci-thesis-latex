	Language = {English},
	Number = {2},
	Pages = {pp. 119-146},
	Publisher = {Springer},
	Title = {Containing the Atom: Sociotechnical Imaginaries and Nuclear Power in the United States and South Korea},
	Url = {http://www.jstor.org/stable/41821489},
	Volume = {47},
	Year = {2009},
	Bdsk-File-1 = {},
