	Month = jan,
	Note = {{ArticleType:} research-article / Full publication date: 1990 / Copyright {\copyright} 1990 Duke University Press},
	Number = {25/26},
	Pages = {56--80},
	Shorttitle = {Rethinking the Public Sphere},
	Title = {Rethinking the Public Sphere: A Contribution to the Critique of Actually Existing Democracy},
	Year = {1990},
